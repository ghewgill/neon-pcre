IMPORT regex

VAR m: regex.Match
IF regex.search("fo+bar", "This is a foobar", OUT m) THEN
    print("\(m[0].start)")
    print("\(m[0].end)")
    print(m[0].group)
END IF
%= 10
%= 16
%= foobar

print(strb(regex.search("asdf", "foobar", OUT m)))
%= FALSE

_ := regex.search("(a|(z))(bc)", "abc", OUT m)
ASSERT m[0].matched = TRUE
ASSERT m[0].start = 0
ASSERT m[0].end = 3
ASSERT m[0].group = "abc"
ASSERT m[1].matched = TRUE
ASSERT m[1].start = 0
ASSERT m[1].end = 1
ASSERT m[1].group = "a"
ASSERT m[2].matched = FALSE
ASSERT m[3].matched = TRUE
ASSERT m[3].start = 1
ASSERT m[3].end = 3
ASSERT m[3].group = "bc"
